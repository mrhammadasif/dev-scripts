{
  "cSpell.words": [
    "antfu",
    "demi",
    "iconify",
    "intlify",
    "pinia",
    "pnpm",
    "regeneratesecret",
    "swaggerfile",
    "unocss",
    "unplugin",
    "Vite",
    "vitejs",
    "Vitesse",
    "vitest",
    "vueuse"
  ],
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.sortKeys": true,
  "prettier.enable": false,
  "eslint.experimental.useFlatConfig": true,
  "editor.formatOnSave": false,
  // Auto fix
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "always",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],
  "files.associations": {
    "*.css": "postcss"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
  "vue.codeActions.enabled": false,
  "testing.automaticallyOpenPeekView": "never",
  "workbench.colorCustomizations": {
    "editorInlayHint.background": "#00000000",
    "editorInlayHint.foreground": "#7e40a5",
    "editorInlayHint.parameterBackground": "#00000000",
    "editorInlayHint.parameterForeground": "#4e5980",
    "tab.activeBackground": "#484367",
    "activityBar.activeBackground": "#8282b8",
    "activityBar.background": "#8282b8",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#7a4545",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#8282b8",
    "statusBar.background": "#6162a6",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#8282b8",
    "statusBarItem.remoteBackground": "#6162a6",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#6162a6",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#6162a699",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#6162a6"
}
